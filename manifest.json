{
	"manifest_version": 2,
	"name": "Playlist time counter",
	"description": "You can count time of any playlist with the help of this extension",
	"version": "1.0.0",
	"icons": {
		"128": "clock.png"
	},
	"content_scripts": [{
		"matches": ["http://www.youtube.com/*","https://www.youtube.com/*"],
		"js": ["index.js"]
	}],
	"background": {
		"scripts":["background.js"]
	},
	"browser_action" : {
		"default_icon" : "clock32.png"
	}
}